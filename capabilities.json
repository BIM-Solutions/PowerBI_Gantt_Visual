                    }
                }
            }
        },
        "legend": {
            "displayName": "Legend",
            "properties": {
                "position": {
                    "displayName": "Position",
                    "type": {
                        "enumeration": [
                            { "value": "top" },
                            { "value": "bottom" },
                            { "value": "left" },
